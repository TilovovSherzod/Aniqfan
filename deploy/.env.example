# Example environment variables for production
# Copy to .env (outside version control) and edit values.

# secret key
DJANGO_SECRET_KEY=replace-with-a-very-long-random-string

# set to 1 for debug (do NOT enable in real production)
DJANGO_DEBUG=0

# comma-separated list of allowed hosts (domain for your site)
# Example for this project:
DJANGO_ALLOWED_HOSTS=aniqqishloqxojaligi.uz,www.aniqqishloqxojaligi.uz

# secure cookie flags (set to 1 when serving over HTTPS)
DJANGO_SESSION_COOKIE_SECURE=1
DJANGO_CSRF_COOKIE_SECURE=1
